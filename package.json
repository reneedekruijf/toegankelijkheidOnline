{
  "name": "parcel-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel serve ./src/views/index.pug --open",
    "build": "parcel build ./src/views/index.pug --no-source-maps && trash dist/*.woff && trash dist/*.woff2"
  },
  "keywords": [],
  "author": "Renee de Kruijf <renee@webdesignpuntnl.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "parcel-bundler": "^1.12.5",
    "prismjs": "^1.23.0"
  },
  "devDependencies": {
    "parcel": "^2.0.0-nightly.688",
    "parcel-plugin-custom-dist-structure": "^1.1.18",
    "parcel-plugin-markdown-it": "^1.0.0",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "pug": "^3.0.2",
    "sass": "^1.34.0"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/assets",
        "staticOutDir": "assets"
      },
      {
        "staticPath": "src/fonts",
        "staticOutDir": "fonts"
      },
      {
        "staticPath": "src/php",
        "staticOutDir": "php"
      }
    ]
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      ".css": "css",
      "images": [
        ".jpg",
        ".png",
        ".svg"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    },
    "options": {
      "development": false
    }
  }
}
